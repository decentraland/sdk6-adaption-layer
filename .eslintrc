{
  "env": {
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/consistent-type-definitions": ["error", "type"]
  },
  "ignorePatterns": ["sdk6-tests", "test-ts", "node_modules"]
}
